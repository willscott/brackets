{
  "manifest_version": 2,
  "name": "Brackets",
  "description": "Brackets",
  "version": "0.0.1",
  "app": {
    "background": {
      "scripts": ["chrome.js"]
    }
  },
  "sandbox": {
	  "pages": ["sandbox.html"]
   },
  "permissions": ["app.window", "experimental", "storage","fileSystem", "unlimitedStorage", "http://dev.brackets.io/"],
  "icons": {"128": "chromeicon.png"}
}
